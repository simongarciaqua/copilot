{
    "version": 2,
    "functions": {
        "api/index.py": {
            "runtime": "@vercel/python",
            "includeFiles": "api/stop_reparto/**",
            "excludeFiles": "frontend/**"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ]
}