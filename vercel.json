{
    "version": 2,
    "functions": {
        "api/*.py": {
            "runtime": "@vercel/python"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/frontend/dist/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/dist/$1",
            "check": true
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "/frontend/dist/index.html"
        }
    ]
}